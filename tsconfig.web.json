{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/preload/*.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.vue",
    "src/renderer/src/types/*.d.ts",
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "types": ["pinia-plugin-persist"],
    "paths": {
      "@renderer/*": ["src/renderer/src/*"]
    },
  }
}
